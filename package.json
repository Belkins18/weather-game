{
  "name": "weather-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
   
    "env-cmd": "^10.1.0",
    "node-sass": "^7.0.3",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "dev": "yarn dev:development",
    "build": "yarn build:production",
    "dev:development": "env-cmd -f .env.development craco start",
    "dev:production": "env-cmd -f .env.production craco start",
    "build:development": "env-cmd -f .env.development craco build",
    "build:production": "env-cmd -f .env.production craco build",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@bem-react/classname": "^1.5.12",
    "@commitlint/config-conventional": "^17.1.0",
    "@emotion/babel-plugin": "^11.10.5",
    "craco-alias": "^3.0.1",
    "eslint": "^8.26.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "iso-3166-ts": "^2.2.0",
    "prettier": "^2.7.1"
  }
}
